{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "dataGroups": [{
    "name": "api-curr-weather",
    "urls": [
      "https://api.openweathermap.org/data/2.5/weather"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "timeout": "5s",
      "maxSize": 5,
      "maxAge": "15m"
    }
  }, {
    "name": "api-forecast",
    "urls": [
      "https://api.openweathermap.org/data/2.5/forecast"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "timeout": "7s",
      "maxSize": 20,
      "maxAge": "3h"
    }
  }, {
    "name": "api-static-files",
    "urls": [
      "http://openweathermap.org/img/wn/**",
      "https://avatars1.githubusercontent.com/u/**"
    ],
    "cacheConfig": {
      "strategy": "performance",
      "maxSize": 100,
      "maxAge": "30d"
    }
  }],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
